{
  "name": "@superblocksteam/javascript",
  "version": "0.0.1",
  "description": "Superblocks JavaScript Plugin",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": "20.16.0",
    "npm": "^10.2.4"
  },
  "scripts": {
    "start": "chokidar '**/*.ts' '../../shared-backend/**/*' -c 'npm run build'",
    "build": "swc src -d dist",
    "clean": "rimraf ./dist *tsbuildinfo",
    "lint": "eslint . --ext ts --max-warnings 0 --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "lint:fix": "eslint . --ext ts --fix --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "lint:precommit": "eslint --ext ts --fix --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "typecheck": "npx tsc",
    "check": "npm run typecheck && npm run lint",
    "test": "jest --coverage --detectOpenHandles",
    "pre-commit": "tsc && lint-staged"
  },
  "dependencies": {
    "@notionhq/client": "2.2.5",
    "@paralleldrive/cuid2": "2.2.0",
    "@superblocks/shared": "workspace:*",
    "amazon-qldb-driver-nodejs": "2.2.0",
    "aws-sdk": "2.1367.0",
    "axios": "^1.6.8",
    "base64url": "3.0.1",
    "bcrypt": "5.1.0",
    "deasync": "0.1.29",
    "esprima": "4.0.1",
    "jmespath": "0.16.0",
    "jsonwebtoken": "9.0.0",
    "lodash": "^4.17.21",
    "minimist": "1.2.6",
    "moment": "2.29.4",
    "notion-to-md": "2.7.0",
    "piscina": "^3.2.0",
    "webflow-api": "1.2.2",
    "xml2json": "0.12.0",
    "xmlbuilder2": "3.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "1.7.9",
    "@swc/jest": "0.2.36",
    "@types/esprima": "^4.0.3",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "chokidar-cli": "3.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.5.4"
  }
}
