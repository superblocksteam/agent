{
  "name": "@superblocks/worker.js",
  "version": "0.0.1",
  "description": "Superblocks Javascript Worker",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start": "chokidar '**/*.ts' '../../../superblocks/packages/shared/dist/**/*' '../../../superblocks/packages/shared-backend/dist/**/*' -c 'npm run build'",
    "clean": "rm -rf ./dist *tsbuildinfo",
    "build": "swc ./src --out-dir ./dist --config-file .swcrc",
    "test": "npm run test:unit && npm run test:unit:transport",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest ./src/*.test.ts ./src/**/*.test.ts ./src/**/**/*.test.ts ./src/test/unit --testPathIgnorePatterns ./src/test/load ./src/test/integration src/*.integration.test.ts --passWithNoTests",
    "test:unit:transport": "jest ./src/test/unit",
    "test:integration": "jest ./src/test/integration/store && jest ./src/test/integration/e2e ./src/test/integration/random --runInBand",
    "test:integration:store": "jest ./src/test/integration/store",
    "test:integration:e2e": "jest ./src/test/integration/e2e",
    "test:integration:e2e:variable": "jest ./src/test/integration/e2e/variable.test.ts",
    "test:load": "jest ./src/test/load",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --fix",
    "typecheck": "npx tsc"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.7.26",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "chokidar-cli": "3.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0",
    "@notionhq/client": "2.2.15",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.25.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.52.1",
    "@opentelemetry/resources": "1.25.1",
    "@opentelemetry/sdk-trace-base": "1.25.1",
    "@opentelemetry/sdk-trace-node": "1.25.1",
    "@opentelemetry/semantic-conventions": "1.25.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@redis/client": "1.5.17",
    "@superblocks/shared": "workspace:*",
    "acorn": "8.12.1",
    "amazon-qldb-driver-nodejs": "2.2.0",
    "aws-sdk": "2.1664.0",
    "axios": "1.8.3",
    "base64url": "3.0.1",
    "bcrypt": "5.1.1",
    "date-fns": "3.6.0",
    "deasync": "0.1.30",
    "dotenv": "16.4.5",
    "esprima": "4.0.1",
    "express": "4.21.2",
    "express-prom-bundle": "6.6.0",
    "generic-pool": "3.9.0",
    "google-protobuf": "3.21.4",
    "heap-js": "2.5.0",
    "jmespath": "0.16.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "moment-timezone": "0.5.46",
    "notion-to-md": "3.1.1",
    "pino": "8.21.0",
    "prom-client": "15.1.3",
    "@superblocksteam/javascript": "workspace:*",
    "vm2": "^3.10.4",
    "web3": "4.11.1",
    "webflow-api": "1.3.1",
    "xml2json": "0.12.0",
    "xmlbuilder2": "3.1.1"
  }
}
