{
  "name": "javascript-plugins-sandbox",
  "version": "1.0.0",
  "description": "JavaScript Plugins gRPC server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "npx rimraf ./dist ./coverage *tsbuildinfo",
    "lint": "eslint . --ext ts --max-warnings 0 --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "lint:fix": "eslint . --ext ts --fix --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "test": "echo $SPEC_PATH && echo $TEST_NAME && jest -i ./${SPEC_PATH} -t ${TEST_NAME}",
    "test:coverage": "echo $SPEC_PATH && echo $TEST_NAME && jest --coverage --coverageReporters=text-summary json-summary lcov -i ./${SPEC_PATH} -t ${TEST_NAME}",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "1.9.0",
    "@superblocks/worker.js": "workspace:*",
    "@superblocksteam/adls": "workspace:*",
    "@superblocksteam/athena": "workspace:*",
    "@superblocksteam/bigquery": "workspace:*",
    "@superblocksteam/cockroachdb": "workspace:*",
    "@superblocksteam/cosmosdb": "workspace:*",
    "@superblocksteam/couchbase": "workspace:*",
    "@superblocksteam/databricks": "workspace:*",
    "@superblocksteam/dynamodb": "workspace:*",
    "@superblocksteam/email": "workspace:*",
    "@superblocksteam/gcs": "workspace:*",
    "@superblocksteam/graphql": "workspace:*",
    "@superblocksteam/gsheets": "workspace:*",
    "@superblocksteam/javascript": "workspace:*",
    "@superblocksteam/javascript-wasm": "workspace:*",
    "@superblocksteam/kafka": "workspace:*",
    "@superblocksteam/kinesis": "workspace:*",
    "@superblocksteam/mariadb": "workspace:*",
    "@superblocksteam/mongodb": "workspace:*",
    "@superblocksteam/mssql": "workspace:*",
    "@superblocksteam/mysql": "workspace:*",
    "@superblocksteam/openai": "workspace:*",
    "@superblocksteam/oracledb": "workspace:*",
    "@superblocksteam/postgres": "workspace:*",
    "@superblocksteam/redis": "workspace:*",
    "@superblocksteam/redshift": "workspace:*",
    "@superblocksteam/restapi": "workspace:*",
