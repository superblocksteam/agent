    "@superblocks/shared": "workspace:*",
    "@superblocksteam/smtp": "workspace:*",
    "@superblocksteam/snowflake": "workspace:*",
    "@superblocksteam/superblocks-ocr": "workspace:*",
    "@superblocksteam/workflow": "workspace:*",
    "google-protobuf": "^3.21.2",
    "dotenv": "16.4.5",
    "ion-js": "5.2.1",
    "jsbi": "4.3.0",
    "pino": "8.21.0",
    "prom-client": "15.1.3"
  },
