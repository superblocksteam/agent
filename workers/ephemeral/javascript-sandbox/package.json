{
  "name": "javascript-sandbox",
  "version": "1.0.0",
  "description": "JavaScript gRPC executor server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm install && npm run lint:fix && tsc",
    "clean": "npx rimraf ./dist ./coverage *tsbuildinfo",
    "lint": "eslint . --ext ts --max-warnings 0 --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "lint:fix": "eslint . --ext ts --fix --cache --cache-strategy content --cache-location ~/.cache/eslint/",
    "test": "echo $SPEC_PATH && echo $TEST_NAME && jest -i ./${SPEC_PATH} -t ${TEST_NAME}",
    "test:coverage": "echo $SPEC_PATH && echo $TEST_NAME && jest --coverage --coverageReporters=text-summary json-summary lcov -i ./${SPEC_PATH} -t ${TEST_NAME}",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "@superblocks/shared": "file:../../javascript/packages/shared",
    "dotenv": "16.4.5",
    "google-protobuf": "^3.21.2",
    "vm2": "^3.10.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "^9.22.0",
    "@jest/globals": "^30.2.0",
    "@side/jest-runtime": "^1.1.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "axios": "^1.6.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prettier": "^3.5.3",
    "rimraf": "6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.26.1"
  }
}
